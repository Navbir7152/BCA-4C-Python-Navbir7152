//WAP to demonstrate built-in functions in Lists

def main():
    my_list = [3, 1, 5, 2, 4]
    print("Original list:", my_list)
    print("Length of the list:", len(my_list))
    print("Sum of the elements:", sum(my_list))
    print("Minimum element:", min(my_list))
    print("Maximum element:", max(my_list))
    print("Sorted list:", sorted(my_list))
    my_list.sort()
    print("Sorted list (after sorting in place):", my_list)

if __name__ == "__main__":
    main()
